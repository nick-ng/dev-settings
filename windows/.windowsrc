## Put in `~/.bashrc`

source ~/.nickrc

## PATHs ##
if [[ ":$PATH:" == *":~/.misc/bin:"* ]]; then
  echo "Misc PATH set correctly"
else
  export PATH=$PATH:"~/.misc/bin"
fi
alias jq="jq-win64" # put in ~/.miscrc - https://github.com/jqlang/jq

if [[ ":$PATH:" == *":~/.cargo/bin:"* ]]; then
  echo "Rust PATH set correctly"
else
  export PATH=$PATH:"~/.cargo/bin"
fi

## Windows Tools ##
alias tree="cmd //c tree"

## Git Bash Stuff ##
source /mingw64/share/git/completion/git-completion.bash
