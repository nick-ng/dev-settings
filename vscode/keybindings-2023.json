[
	{ "key": "ctrl+alt+f", "command": "editor.action.formatSelection" },
	{
		"key": "ctrl+u",
		"command": "-cursorUndo",
		"when": "textInputFocus"
	},
	{
		"key": "ctrl+u ctrl+u",
		"command": "editor.action.transformToUppercase"
	},
	{
		"key": "ctrl+u ctrl+l",
		"command": "editor.action.transformToLowercase"
	},
	{
		"key": "shift+ctrl+f",
		"command": "search.action.openNewEditor"
	},
	{
		"key": "ctrl+alt+s",
		"command": "workbench.action.navigateLeft"
	},
	{
		"key": "ctrl+alt+f",
		"command": "workbench.action.navigateRight"
	},
	{
		"key": "ctrl+alt+e",
		"command": "workbench.action.navigateUp"
	},
	{
		"key": "ctrl+alt+d",
		"command": "workbench.action.navigateDown"
	},
	{
		"key": "ctrl+i",
		"command": "-editor.action.triggerSuggest",
		"when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly && !suggestWidgetVisible"
	},
	{
		"key": "ctrl+i",
		"command": "editor.action.showHover",
		"when": "editorTextFocus"
	},
	{
		"key": "escape",
		"command": "-cancelSelection",
		"when": "editorHasSelection && textInputFocus"
	},
	{
		"key": "ctrl+alt+p",
		"command": "extension.nickToggleUseIgnoreFiles"
	},
	{
		"key": "ctrl+`",
		"command": ""
	},
	{
		"key": "ctrl+7",
		"command": "extension.nickGoToBookmark0"
	},
	{
		"key": "ctrl+8",
		"command": "extension.nickGoToBookmark1"
	},
	{
		"key": "ctrl+9",
		"command": "extension.nickGoToBookmark2"
	},
	{
		"key": "ctrl+0",
		"command": "extension.nickGoToBookmark3"
	},
	{
		"key": "ctrl+u ctrl+7",
		"command": "extension.nickSetBookmark0"
	},
	{
		"key": "ctrl+u ctrl+8",
		"command": "extension.nickSetBookmark1"
	},
	{
		"key": "ctrl+u ctrl+9",
		"command": "extension.nickSetBookmark2"
	},
	{
		"key": "ctrl+u ctrl+0",
		"command": "extension.nickSetBookmark3"
	},
	{
		"key": "ctrl+7",
		"command": "-workbench.action.focusSeventhEditorGroup"
	},
	{
		"key": "ctrl+8",
		"command": "-workbench.action.focusEighthEditorGroup"
	},
	{
		"key": "ctrl+9",
		"command": "-workbench.action.lastEditorInGroup"
	},
	{
		"key": "ctrl+3",
		"command": "workbench.action.focusSideBar"
	},
	{
		"key": "ctrl+0",
		"command": "-workbench.action.focusSideBar"
	},
	{
		"key": "ctrl+3",
		"command": "-workbench.action.focusThirdEditorGroup"
	}
]
